{
      "name": "discord",
      "version": "1.0.18",
      "description": "A Electron Powered Discord Client For Older Systems",
      "main": "main.js",
      "scripts": {
        "start": "electron .",
        "build": "electron-builder --win --x64 --ia32",
        "dist": "electron-builder"
      },
      "keywords": [
        "Electron",
        "Discord",
        "Wrapper"
      ],
      "author": "Kai (Magic Dippy Egg)",
      "license": "MIT",
      "devDependencies": {
        "electron": "^22.0.0",
        "electron-builder": "^24.0.0"
      },
      "build": {
        "appId": "com.magicdippyegg.discord",
        "productName": "Discord",
        "copyright": "Copyright Â© 2025 ${author}",
        "directories": {
          "output": "dist"
        },
        "files": [
          "main.js",
          "package.json",
          "icon.ico",
          "preload.js"
        ],
        "win": {
          "target": [
            "nsis",
            "zip"
          ],
          "icon": "icon.ico"
        },
        "nsis": {
          "installerIcon": "icon.ico",
          "uninstallerIcon": "icon.ico",
          "uninstallDisplayName": "Discord Uninstaller",
          "license": "LICENSE.md",
          "allowToChangeInstallationDirectory": true,
          "oneClick": false
        }
      }
    }